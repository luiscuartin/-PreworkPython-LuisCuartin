def calcular_precio_final():
        precio_original = float(input("Ingresa el precio original del artículo: "))

        descuento = 0.2
        monto_descuento = precio_original * descuento
        precio_final = precio_original - monto_descuento

        print(f"El precio final después de aplicar un descuento del 20% es: {precio_final} euros.")

calcular_precio_final()
